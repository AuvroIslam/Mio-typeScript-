{
  "expo": {
    "name": "Mio",
    "slug": "Mio",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/finallogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/finallogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mioapp.social",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/finallogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mioapp.social",
      "versionCode": 7,     
       "permissions": [
        "INTERNET",
        "POST_NOTIFICATIONS",
        "VIBRATE"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/finallogo.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      [
        "@react-native-google-signin/google-signin"
       
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "867d3cc1-2a00-4cd5-8348-6d2dfacdc9c6"
      }
    },
    "runtimeVersion": "1.0.5",
    "owner": "auvro2002",
    "updates": {
      "url": "https://u.expo.dev/867d3cc1-2a00-4cd5-8348-6d2dfacdc9c6"
    }
  }
}
